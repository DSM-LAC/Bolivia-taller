setwd("~/Downloads/NoSilverBulletOnDSM-2.0/Peru")

library(rgdal)
library(raster)
#CARGA LOS DATOS
dat <- read.csv("alldata_Peru.csv")
#LIMITE DE PAIS
#QUITA NA
dat <- na.omit(dat)
#GENERA ID COLUMN
dat$IDPROF <- paste0("IDPROF_", dat$longitude, "_", dat$latitud)

#SELECCIONA SOLAMENTE LOS DATOS SUPERFICIALES
dat0 <- dat[dat$top==0,]
dat0 <- dat0[c(2,3,6)]
str(dat0)





